<h1>New Poll</h1>
<br>
<%= form_for @poll do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true %>
  <br>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
  <br>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <br>
<%= f.submit  class: "btn btn-primary" %>
<% end %>


